#!/usr/bin/env bash
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )"/.. >/dev/null && pwd )"
set -a
source ${DIR}/conf/.env

docker-compose -f ${DIR}/conf/docker-compose.yml -p ${PROJECT_NAME} up -d --build --force-recreate

echo "***************************"
echo "Hostname: ${HOST_NAME}"
echo "***************************"
